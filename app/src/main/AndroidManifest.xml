<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="brandon.example.com.iotecapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CreateAccountActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".ResetPasswordActivity" />
        <activity android:name=".PedirMaterialesActivity" />
        <activity android:name=".PerfilActivity" />
        <activity
            android:name=".AcercaDeActivity"
            android:label="@string/title_activity_acerca_de"
            android:theme="@style/AppTheme" />
        <activity android:name=".DispositivosActivity" />
        <activity android:name=".MisPrestamosActivity"></activity>
    </application>

</manifest>